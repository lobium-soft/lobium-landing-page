include ../logo/_logo
include _navigation

header.section.page-header
	//RD Navbar
	.rd-navbar-wrap
		nav.rd-navbar
			.rd-navbar-main-outer
				.rd-navbar-main

					.rd-navbar-nav-wrap.rd-navbar-main-element-1
						+navigation( menuDigital )

					//RD Navbar Panel
					.rd-navbar-panel
						//RD Navbar Toggle
						button.rd-navbar-toggle( data-rd-navbar-toggle='.rd-navbar-nav-wrap' ): span

						//RD Navbar Brand
						.rd-navbar-brand
							+logo

					.rd-navbar-collapse.rd-navbar-main-element-1.text-lg-end
						.h6 P:
							=' '
							a( href= 'tel:#' ) +1 234 567 8901
			.rd-navbar-collapse-toggle.rd-navbar-fixed-element-1( data-rd-navbar-toggle='.rd-navbar-collapse' ): span
